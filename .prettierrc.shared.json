{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "vueIndentScriptAndStyle": true,
  "printWidth": 100,
  "overrides": [
    {
      "files": "*.py",
      "options": {
        "printWidth": 88,
        "tabWidth": 4
      }
    },
    {
      "files": ["*.md", "*.yml", "*.yaml"],
      "options": {
        "printWidth": 80
      }
    }
  ]
}