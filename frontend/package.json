{
  "name": "aris",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "test": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs",
    "test:run": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs run",
    "test:browser": "vitest --config vitest.browser.config.js",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:e2e": "playwright test",
    "test:e2e:all": "CI=true playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test --grep @visual",
    "test:all": "npm run test:run && npm run test:e2e"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.6",
    "@openai/codex": "^0.1.2504301751",
    "@tabler/icons-vue": "^3.34.0",
    "@vueuse/core": "^13.4.0",
    "@yaireo/relative-time": "^1.0.5",
    "axios": "^1.10.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/eslint-parser": "^7.27.5",
    "@babel/plugin-syntax-import-assertions": "^7.26.0",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@eslint/js": "^9.29.0",
    "@playwright/test": "^1.53.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-vue": "~10.2.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.1",
    "prettier": "^3.5.3",
    "puppeteer": "^24.10.2",
    "puppeteer-core": "^24.10.2",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.1.3"
  }
}
