{
  "name": "aris",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node ../docker/env-check.js && vite",
    "build": "node ../docker/env-check.js && vite build",
    "preview": "node ../docker/env-check.js && vite preview",
    "lint": "eslint . --fix",
    "test": "node ../docker/env-check.js && node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs",
    "test:run": "node ../docker/env-check.js && node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs run --silent",
    "test:browser": "node ../docker/env-check.js && vitest --config vitest.browser.config.js",
    "test:coverage": "node ../docker/env-check.js && vitest run --coverage",
    "test:coverage:ui": "node ../docker/env-check.js && vitest --coverage --ui",
    "test:e2e": "node ../docker/env-check.js && node --max-old-space-size=2048 ./node_modules/playwright/cli.js test --reporter=line --quiet --grep-invert '@mobile-only'",
    "test:e2e:all": "node ../docker/env-check.js && CI=true playwright test",
    "test:e2e:ui": "node ../docker/env-check.js && playwright test --ui",
    "test:e2e:debug": "node ../docker/env-check.js && playwright test --debug",
    "test:visual": "node ../docker/env-check.js && playwright test --grep @visual",
    "test:all": "npm run test:run && npm run test:e2e",
    "storybook": "node ../docker/env-check.js && storybook dev -p ${STORYBOOK_PORT}",
    "build-storybook": "node ../docker/env-check.js && storybook build"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.6",
    "@openai/codex": "^0.1.2504301751",
    "@tabler/icons-vue": "^3.34.0",
    "@vueuse/core": "^13.4.0",
    "@yaireo/relative-time": "^1.0.5",
    "axios": "^1.10.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/eslint-parser": "^7.27.5",
    "@babel/plugin-syntax-import-assertions": "^7.26.0",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@eslint/js": "^9.29.0",
    "@playwright/test": "^1.53.1",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-viewport": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/vue3-vite": "^8.6.14",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-storybook": "^9.1.1",
    "eslint-plugin-vue": "~10.2.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.1",
    "prettier": "^3.5.3",
    "puppeteer": "^24.14.0",
    "puppeteer-core": "^24.10.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.6.14",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.1.3"
  }
}
